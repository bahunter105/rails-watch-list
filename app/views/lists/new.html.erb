

<div class="container">
  <div class="row">
    <div class="col-3"></div>
    <div class="col-6">
      <h2>Add New List: </h2>
      <%= simple_form_for @list do |f| %>
        <%= f.input :name %>
        <%= f.input :photo, as: :file %>
        <%= f.button :submit, class: "btn-flat"%>
      <% end %>
    </div>
    <div class="col-3"></div>
  </div>
  <div class="row"></div>
  <div class="row"></div>
</div>

<div class="cards d-flex justify-items-center">
</div>
